{
  "name": "meli",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apicache": "^1.1.0",
    "bootstrap": "3",
    "express": "^4.16.2",
    "fetch-ponyfill": "^4.1.0",
    "history": "^4.7.2",
    "morgan": "^1.9.0",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.1.1",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-scripts": "1.0.17",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "build:css": "node-sass-chokidar src/ -o src/",
    "watch:css": "npm run build:css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "build:server": "rm -Rf ./build/server && ./node_modules/.bin/babel --presets 'es2015,stage-2' ./server -d ./build/server",
    "start:server": "yarn build:server && exec node ./build/server/index.js",
    "start:client": "react-scripts start",
    "start": "npm-run-all -p watch:css start:server start:client",
    "build:js": "react-scripts build",
    "build": "rm -Rf ./build && npm-run-all build:js build:server build:css",
    "test": "react-scripts test --env=jsdom",
    "lint": "eslint --ext js,jsx ."
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "redux-devtools": "^3.4.1"
  }
}
